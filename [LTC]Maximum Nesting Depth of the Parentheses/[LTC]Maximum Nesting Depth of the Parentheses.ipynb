   "source": [
    "class Solution:\n",
    "    def maxDepth(self, s: str) -> int:\n",
    "        count = 0 \n",
    "        m = 0\n",
    "        for i in s:\n",
    "            if i == '(': \n",
    "                count +=1\n",
    "            if i == ')':\n",
    "                count -=1\n",
    "            elif count > m:\n",
    "                m = count\n",
    "        return m"
   ]
